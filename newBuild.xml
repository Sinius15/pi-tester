<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project pi">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="pi.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="com.sinius15.pi.PiServer"/> 
                <attribute name="Class-Path" value="."/>
            </manifest>
            <zipfileset excludes="META-INF/*.SF" src="libs/pi4j/pi4j-gpio-extension.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="libs/pi4j/pi4j-device.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="libs/pi4j/pi4j-core.jar"/>
        	<zipfileset excludes="META-INF/*.SF" src="libs/Java-Launchpad-Library_v2.0.jar"/>
            <fileset dir="bin"/>
            <zipfileset excludes="META-INF/*.SF" src="libs/pi4j/pi4j-example.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="libs/pi4j/pi4j-service.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="libs/jline-1.0/jline-1.0.jar"/>
        </jar>
    </target>
</project>
